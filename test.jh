entity Owner {
	name String
}
entity Car {
	model String,
    year String
}

relationship OneToMany {
  Owner{car} to Car{owner}
}

relationship ManyToOne {
  Car{ownedBy} to Owner
}

entity GIDUser {
	firstName String,
	lastName String,
}

entity GIDIdentity {
	gid String,
	pgid String,
}

entity GIDMembership {
	ogid String,
	tenantKey String,
	tenantUserKey String,
    tenantUserBlock String,
}

entity GIDMoniker {
	moniker String,
	prefix String,
    prefix GIDMonikerPrefix
}

enum GIDMonikerPrefix {
    LEGACY, TEL, MAILTO
}

entity GIDMonikerSet {
}

relationship OneToOne {
    GIDUser{monickers} to GIDMonikerSet

	GIDIdentity{monickers} to GIDMonikerSet
    GIDIdentity{fullMonikerSet} to GIDMonikerSet
    GIDIdentity{standardMonikerSet} to GIDMonikerSet

	GIDMembership{monickers} to GIDMonikerSet
}

relationship OneToMany {
	GIDUser{identities} to GIDIdentity{user},
	GIDIdentity{memberships} to GIDMembership{identity}
}

relationship ManyToMany {
	GIDMonikerSet{monikers(moniker)} to GIDMoniker{set}
}
// Set pagination options
paginate GIDUser, GIDIdentity, GIDMembership, GIDMoniker with infinite-scroll

// Use Data Transfert Objects (DTO)
// dto * with mapstruct

