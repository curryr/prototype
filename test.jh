entity GIDUser {
	firstName String,
	lastName String,
}

entity GIDIdentity {
	gid String,
	pgid String,
}

entity GIDMembership {
	ogid String,
	tenantKey String,
	tenantUserKey String,
    tenantUserBlock String,
}

entity GIDMoniker {
	moniker String,
	prefix String,
    prefix GIDMonikerPrefix
}

enum GIDMonikerPrefix {
    LEGACY, TEL, MAILTO
}

entity GIDMonikerSet {
}

relationship OneToOne {
    GIDUser{monickers} to GIDMonikerSet

	GIDIdentity{monickers} to GIDMonikerSet
    GIDIdentity{fullMonikerSet} to GIDMonikerSet
    GIDIdentity{standardMonikerSet} to GIDMonikerSet

	GIDMembership{monickers} to GIDMonikerSet
}

relationship OneToMany {
	GIDUser to GIDIdentity{user},
	GIDIdentity to GIDMembership{identity}
}

relationship ManyToMany {
	GIDMonikerSet to GIDMoniker
}
// Set pagination options
paginate GIDUser, GIDIdentity, GIDMembership, GIDMoniker with infinite-scroll

// Use Data Transfert Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
//service all with serviceImpl except Employee, Job

